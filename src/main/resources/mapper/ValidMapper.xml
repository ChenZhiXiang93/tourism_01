<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bonc.mapper.ValidMapper">
    <select id="getTourismId" parameterType="java.lang.String" resultType="java.lang.String">
        select tourism_id as tourismId from TOURISM_USER_PERMISSION where code = #{code};
    </select>
</mapper>